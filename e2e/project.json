{
  "name": "e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nxkit/playwright:test",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "outputPath": "dist/e2e/test-results",
        "playwrightConfig": "e2e/playwright.config.ts",
        "devServerTarget": "website:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "website:serve:production"
        }
      }
    },
    "debug": {
      "executor": "@nxkit/playwright:test",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "outputPath": "dist/e2e/test-results",
        "playwrightConfig": "e2e/playwright.config.ts",
        "devServerTarget": "website:serve:development",
        "debug": true
      },
      "configurations": {
        "production": {
          "devServerTarget": "website:serve:production"
        }
      }
    },
    "show-report": {
      "executor": "@nxkit/playwright:show-report",
      "options": {
        "reportPath": "dist/e2e/playwright-report"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["website"]
}
